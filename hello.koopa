decl @getint(): i32

decl @getch(): i32

decl @getarray(*i32): i32

decl @putint(i32)

decl @putch(i32)

decl @putarray(i32, *i32)

decl @starttime()

decl @stoptime()

fun @main(): i32 {
%entry:
  %retval = alloc i32
  @x = alloc i32
  jump %0

%0:
  store 1, @x
  %1 = load @x
  store %1, %retval
  jump %exit

%2:
  jump %exit

%exit:
  %3 = load %retval
  ret %3
}
