fun @main(): i32 {
%entry:
  %retval = alloc i32
  @i = alloc i32
  jump %0

%0:
  store 0, @i
  jump %while_entry

%while_entry:
  %1 = load @i
  %2 = lt %1, 10
  br %2, %while_body, %while_exit

%while_body:
  store 20, @i
  jump %while_body

%3:
  %4 = load @i
  %5 = add %4, 1
  store %5, @i
  jump %while_entry

%while_exit:
  %6 = load @i
  store %6, %retval
  jump %exit

%7:
  jump %exit

%exit:
  %8 = load %retval
  ret %8
}
