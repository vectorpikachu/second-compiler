fun @add(%0: i32, %1: i32): i32 {
%entry:
  %retval = alloc i32
  @a = alloc i32
  @b = alloc i32
  jump %2

%2:
  store %0, @a
  store %1, @b
  %3 = load @a
  %4 = load @b
  %5 = add %3, %4
  store %5, %retval
  jump %exit

%6:
  jump %exit

%exit:
  %7 = load %retval
  ret %7
}

fun @main(): i32 {
%entry:
  %retval = alloc i32
  jump %8

%8:
  %9 = call @add(1, 2)
  store %9, %retval
  jump %exit

%10:
  jump %exit

%exit:
  %11 = load %retval
  ret %11
}
